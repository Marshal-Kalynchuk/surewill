<% @delegates.each do |delegate| %>
  <% if delegate.saved_changes %>
    <%= turbo_stream.update "#{dom_id(delegate)}_item" do %>
      <%= render "delegate", delegate: delegate %>
    <% end %>
  <% end %>
<% end %>
<%= turbo_stream.remove "#{dom_id(@delegate)}_form" %>
<% @update_properties.each do |property| %>
  <%= turbo_stream.update "#{dom_id(property)}_item" do %>
    <%= render "properties/property", property: property %>
  <% end %>
<% end %>