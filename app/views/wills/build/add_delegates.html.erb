<h3>Delegates</h3>
<%= render "errors" %>

<div class="container">
  <div class="header">
    <h1>Delegates</h1>
  </div>

  <%= turbo_frame_tag "delegates" do %>
    <%= render partial: "delegates/delegate", collection: @delegates %>
  <% end %>
  <%= turbo_frame_tag Delegate.new %>
  <%= link_to "New delegate",
                new_user_will_delegate_path,
                class: "btn btn--primary",
                data: { turbo_frame: dom_id(Delegate.new) } %>
</div>

<%= button_to "Next", wizard_path, method: :put, params: {will: {status: @will.status}} %>
<%= button_to "Back", previous_wizard_path, method: :get %>