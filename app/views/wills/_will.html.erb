<section class='testator'>
  <h3>Testator</h3>
  <p><%= @will.testator %></p>
</section>
<section class='benficiaries'>
  <h2>Beneficiaries</h2>
  <ul>
    <% @beneficiaries.each do |beneficiary| %>
      <li>
        <h3><%= beneficiary.name %></h3>
        <p><%= beneficiary.role %></p>
      </li>
    <% end %>
  </ul>
</section>
<section class='assets'>
  <h2>Assets:</h2>
  <ul>
    <% @assets.each do |asset| %>
      <li>
        <h3><%= asset.title%></h3>
        <p><%= asset.description%></p>
      </li>
    <% end %>
  </ul>
</section>
